{
  "name": "solitaire",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "react-scripts test"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/.{js,jsx,ts,tsx}",
      "!src/*/.d.ts",
      "!src/**/*.stories.tsx"
    ],
    "moduleNameMapper": {
      "\\.svg\\?url": "<rootDir>/src/__tests__/__mocks__/mock-svg.svg",
      "^@public/(.*)$": "<rootDir>/public/$1",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@app/(.*)$": "<rootDir>/src/app/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1"
    }
  },
